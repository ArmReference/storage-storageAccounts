{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "type": "string",
      "metadata": {
        "description": "The resource name."
      }
    },
    "location": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]",
      "metadata": {
        "description": "Resource location."
      }
    },
    "sku": {
      "type": "object",
      "metadata": {
        "description": "The SKU of the storage account."
      }
    },
    "kind": {
      "type": "string",
      "allowedValues": [
        "BlobStorage",
        "BlockBlobStorage",
        "FileStorage",
        "Storage",
        "StorageV2"
      ],
      "metadata": {
        "desciption": "Indicates the type of storage account."
      }
    },
    "extendedLocation": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "The complex type of the extended location."
      }
    },
    "identity": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "Identity for the resource."
      }
    },
    "properties": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "The parameters used to create the storage account."
      }
    },
    "tags": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "Resource tags."
      }
    },
    "DependsOn": {
      "type": "array",
      "defaultValue": [],
      "metadata": {
        "description": "Pass dependencies"
      }
    }
  },
  "variables": {
    "storageAccounts": {
      "type": "Microsoft.Storage/storageAccounts",
      "name": "[parameters('name')]",
      "apiVersion": "2021-08-01",
      "location": "[parameters('location')]",
      "dependsOn": "[if(empty(parameters('DependsOn')),json('null'),parameters('DependsOn'))]",
      "sku": "[parameters('sku')]",
      "kind": "[parameters('kind')]",
      "extendedLocation": "[parameters('extendedLocation')]",
      "identity": "[parameters('identity')]",
      "properties": "[parameters('properties')]",
      "tags": "[parameters('tags')]"
    }
  },
  "resources": [
  ],
  "outputs": {
    "storageAccounts": {
      "type": "object",
      "value": "[variables('storageAccounts')]"
    }
  }
}