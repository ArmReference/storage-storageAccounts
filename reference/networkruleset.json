{
 "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
 "contentVersion": "1.0.0.0",
 "parameters": {
  "bypass": {
   "type": "string",
   "defaultValue": "None",
   "allowedValues": [
    "AzureServices",
    "Logging",
    "Metrics",
    "None"
   ],
   "metadata": {
    "description": "Specifies whether traffic is bypassed for Logging/Metrics/AzureServices. Possible values are any combination of Logging,Metrics,AzureServices (For example, 'Logging, Metrics'), or None to bypass none of those traffics."
   }
  },
  "defaultAction": {
   "type": "string",
   "allowedValues": [
    "Allow",
    "Deny"
   ],
   "metadata": {
    "description": "Specifies the default action of allow or deny when no other rules match."
   }
  },
  "ipRules": {
   "type": "array",
   "defaultValue": [],
   "metadata": {
    "description": "Sets the IP ACL rules"
   }
  },
  "resourceAccessRules": {
   "type": "array",
   "defaultValue": [],
   "metadata": {
    "description": "Sets the resource access rules."
   }
  },
  "virtualNetworkRules": {
   "type": "array",
   "defaultValue": [],
   "metadata": {
    "description": "TSets the virtual network rules"
   }
  }
 },
 "variables": {
  "NetworkRuleSet": {
   "bypass": "[parameters('bypass')]",
   "defaultAction": "[parameters('defaultAction')]",
   "ipRules": "[parameters('ipRules')]",
   "resourceAccessRules": "[parameters('resourceAccessRules')]",
   "virtualNetworkRules": "[parameters('virtualNetworkRules')]"
  }
 },
 "resources": [
 ],
 "outputs": {
  "NetworkRuleSet": {
   "type": "object",
   "value": "[variables('NetworkRuleSet')]",
   "metadata": {
    "description": "NetworkRuleSet"
   }
  }
 }
}