  "contentVersion": "1.0.0.0",
  "parameters": {
    "blob": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "A service that allows server-side encryption to be used."
      }
    },
    "file": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "A service that allows server-side encryption to be used."
      }
    },
    "queue": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "A service that allows server-side encryption to be used."
      }
    },
    "table": {
      "type": "object",
